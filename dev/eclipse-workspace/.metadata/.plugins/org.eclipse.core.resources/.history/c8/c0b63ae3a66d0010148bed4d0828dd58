package sec01_lambdaexpression.EX09_RefOfArrayConstructor_1;

interface A {
	B abc();
}
class B {
	B() {
		System.out.println("첫 번째 생성자");
	}
	B(int k){
		System.out.println("두 번째 생성자");
	}
}
public class RefOfArrayConstructor_1 {
	public static void main(String[] args) {
		// 1. anonymous inner class
		A a1 = new A() {
			@Override
			public B abc() {
				return new B();
			}
		};

		// 2. lambda
		A a2 = ()-> new B();

		// 3. class constructor reference
		A a3 = B::new;
		a1.abc();
		a2.abc();
		a3.abc();
	}
}
