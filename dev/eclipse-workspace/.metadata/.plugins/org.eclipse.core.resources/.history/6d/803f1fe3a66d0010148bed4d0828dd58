package operation1;

public class OpTest2 {

	public static void main(String[] args) {
		boolean b1= true, b2 = false;
		System.out.println(b1 && b2);
		System.out.println(b1 || b2);
		System.out.println(!b1);

		int n1=5, n2=3;
		System.out.println(n1 & n2); // 101 & 011 둘 다 1 -> 001 1
		System.out.println(n1 | n2); // 101 | 011 둘 중 하나 1 -> 111 7
		System.out.println(~n1);
		//일반적인 치환
		/*int temp = n1;
		n1 = n2;
		n2 = temp;*/
		//XOR 연산자로 치환
		/* n1 = n1 ^ n2;
		n2 = n2 ^ n1;
		n1 = n1 ^ n2; */
		System.out.println("종료");
	}

}
