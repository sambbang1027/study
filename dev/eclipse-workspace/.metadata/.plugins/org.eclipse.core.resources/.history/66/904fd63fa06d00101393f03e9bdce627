package day5;

import java.util.*;

public class day5 {
	public static void main (String[] args) {
		
//		int t;
//		int n;
//		int 몫 = n; 
//		for(t만큼){
//
//		몫 /= 2 나머지 = 몫 %=2
//
//		if( 몫 = 1) break }
		
		Scanner sc = new Scanner(System.in);
		
		int t = sc.nextInt();
		int n = sc.nextInt();
		
		int result = n;
		int location = 0;
		
		for(int i = 0; i < t; i++) {
			result /=2;
			
			if(result == 1 ) {
				break;
			}
			if ( (result % 2) == 1) {
				System.out.print(location+ " ");
			}
			location++;
		}

	}
}
