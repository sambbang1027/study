package method;

public class MethodTest6 {
	public static void main(String[] args) {
		int[] a = {10};

		callref(a);
		System.out.println("main : "+a[0]);
		System.out.println("종료");
	}

	// call by reference :배열은 주솟값을 참조하고 복제했을 때
	// 주솟값을 복제한다. 따라서,
	// 안의 value가 바뀌면 함께 바뀐다.

	private static void callref(int[] ary) {
		System.out.println("callref : "+ary[0]);
		ary[0]++;
		System.out.println("callref : "+ary[0]);
	}

}
