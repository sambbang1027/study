package oop11;

public class HarmanSpeaker implements Speaker{

	public HarmanSpeaker () {
		System.out.println("HarmanSpeaker : 제품이 설정됨");
	}

	@Override
	public void soundUp() {
		System.out.println("HarmanSpeaker : 소리를 키웁니다.");
	}

	@Override
	public void soundDown() {
		System.out.println("HarmanSpeaker : 소리를 줄입니.");

	}

}
