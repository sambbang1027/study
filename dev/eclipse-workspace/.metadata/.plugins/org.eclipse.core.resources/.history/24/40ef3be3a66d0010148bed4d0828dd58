package sec05_superkeywordsupermethod.EX03_SuperMethod_1;

class A {
	A(){
		System.out.println("A 생성자");
	}
}

class B extends A {
	B(){
		super(); // 부모 클래스 생성자호출
		System.out.println("B 생성자");
	}
}

class C {
	C(int a){
		System.out.println("C 생성자");
	}
}

class D extends C {
//	D(){
//		super();
//	}
//
	D(){
		super(3);
	}
}
// C는 default Constructor가 없기 때문에 명시적으로 값을 줘야함

public class SuperMethod_1 {
	public static void main(String[] args) {
		// A instance
		A aa = new A();
		System.out.println();

		// B instance
		B bb = new B();
		System.out.println();
	}
}
